{
  "manifest_version": 2,
  "name": "Ms SearchBot, Go-Adler",
  "version": "1.0",
  "description": "Your extension description",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popUp.js"]
    }
  ]
}
